{
    "name": "dhii/iterator-abstract",
    "description": "Functionality for iterators that comply with dhii/iterator-interface, or are otherwise based on iteration objects",
    "type": "library",
    "license": "MIT",
    "keywords": ["iteration", "iterator", "abstract", "trait"],
    "authors": [
        {
            "name": "Dhii Team",
            "email": "development@dhii.co"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.4 | ^7.0",
        "dhii/stringable-interface": "^0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "ptrofimov/xpmock": "^1.1",
        "dhii/php-cs-fixer-config": "^0.1",
        "codeclimate/php-test-reporter": "<=0.3.2",
        "dhii/iterator-interface": "dev-release/0.1"
    },
    "autoload": {
        "psr-4": {
            "Dhii\\Iterator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dhii\\Iterator\\UnitTest\\": "test/unit",
            "Dhii\\Iterator\\FuncTest\\": "test/functional"
        }
    },
    "scripts": {
        "test": "phpunit",
        "csfix": "php-cs-fixer fix -vvv"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.1.x-dev"
        }
    }
}
